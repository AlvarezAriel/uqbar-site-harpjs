body
  font-family "Roboto", sans-serif

.jssora21l, .jssora21r {
  display: block;
  position: absolute;
  /* size of arrow element */
  width: 55px;
  height: 55px;
  cursor: pointer;
  background: url('/assets/vendors/jssor-slider/img/a21.png') center center no-repeat;
  overflow: hidden;
}

.jssora21l {
  background-position: -3px -33px;
}

.jssora21r {
  background-position: -63px -33px;
}

.jssora21l:hover {
  background-position: -123px -33px;
}

.jssora21r:hover {
  background-position: -183px -33px;
}

.jssora21l.jssora21ldn {
  background-position: -243px -33px;
}

.jssora21r.jssora21rdn {
  background-position: -303px -33px;
}

.jssorb21 {
  position: absolute
  div, .jssorb21 div:hover, .jssorb21 .av {
    position: absolute;
    width: 19px;
    height: 19px;
    text-align: center;
    line-height: 19px;
    color: white;
    font-size: 12px;
    background: url('/assets/vendors/jssor-slider/img/b21.png') no-repeat;
    overflow: hidden;
    cursor: pointer;
  }
}

.jssorb21 div {
  background-position: -5px -5px;
}

.jssorb21 div:hover, .jssorb21 .av:hover {
  background-position: -35px -5px;
}

.jssorb21 .av {
  background-position: -65px -5px;
}

.jssorb21 .dn, .jssorb21 .dn:hover {
  background-position: -95px -5px;
}

.people {
  position: absolute;
  width: 700px;
  height: 118px;
  top: 110px;
  left: 133px;
  padding: 5px;
  text-align: left;
  line-height: 53px;
  text-transform: uppercase;
  font-size: 40px;
  color: #fff;
}

.parallax-window {
  min-height: 400px;
  background: transparent;
}

.people-image {
  background-image: url('/assets/images/people.jpg')
  background-position: 50% 50%
  background-repeat: no-repeat
  background-attachment: fixed
  width: 100%
  height: 100%
}

.navbar li a {
  text-transform: uppercase
  font-size 12px
  color #202020 !important
}

.pillars{
  .pillar-icon{
    background-image: url('/assets/images/pillar_icon.png')
    background-size contain
    background-repeat: no-repeat
    background-position: 50% 50%
    height 150px
    width  100%
  }

  div {
    margin-top 30px
    padding-left 50px
    padding-right 50px
  }

  h3 {
    text-align: center;
  }
}

.banner-wisit {
  background-image: url('/assets/images/uqbar_people.jpg')
  background-size cover
  margin-top 50px
  background-repeat: no-repeat
  background-attachment: fixed
  width: 100%
  height: 250px

  .wisit-logo {
    background-image: url("/assets/images/wisit15.png");
    background-repeat: no-repeat;
    background-position: 50% 50%;
    height: 100%;
    width: 100%;
    background-size: 300px
  }
}

.note {
  margin-top 70px
  padding-top 30px
  h3 {
    margin-top -5px
    line-height: 1.2em;
    letter-spacing: 2px;
    font-weight: 800;
    text-transform: uppercase;
  }
  p {
    padding-right 10px
  }
}

.featured {

  .video {
    padding-left 20px
    text-align left
  }

}